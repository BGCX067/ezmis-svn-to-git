<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd"
	default-autowire="byName" default-lazy-init="true">

	
	
	<!-- 自定义表单 -->
	<bean id="cformManager" class="com.jteap.form.cform.manager.CFormManager" />
	<bean id="cformCatalogManager" class="com.jteap.form.cform.manager.CFormCatalogManager" />
	<bean id="cformDataSourceManager" class="com.jteap.form.cform.manager.CFormDataSourceManager" />
	<bean id="cformExcelItemManager" class="com.jteap.form.cform.manager.CFormExcelItemManager" />

	<bean id="form_CFormCatalogAction" class="com.jteap.form.cform.web.CFormCatalogAction" scope="prototype"></bean>
	<bean id="form_CFormAction" class="com.jteap.form.cform.web.CFormAction" scope="prototype"></bean>
	
	
	
</beans>